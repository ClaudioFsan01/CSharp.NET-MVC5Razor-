@model Mvc5RazorAutenticacao.Models.Professores
@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Cadastrar Professor</h2>

@using (Html.BeginForm())
{
    <div class="form-horizontal">

        <div class="form-group">

            <div>
                @Html.DisplayNameFor(model => model.profCodigo)
                @Html.EditorFor(model => model.profCodigo)
            </div>

        </div>

        <div class="form-group">

            <div>
                @Html.DisplayNameFor(model => model.profNome)
                @Html.EditorFor(model => model.profNome)
            </div>
        </div>

        <div class="form-group">
            <div>
                @Html.DisplayNameFor(model => model.profCurso)
               @Html.EditorFor(model => model.profCurso)
           </div>
        </div>


        <div class="form-group">
            <div>
                @Html.DisplayNameFor(model => model.profPreco)
                @Html.EditorFor(model => model.profPreco)
            </div>
        </div>

        <button type="submit" name="button"> Gravar </button>

    </div>


}

